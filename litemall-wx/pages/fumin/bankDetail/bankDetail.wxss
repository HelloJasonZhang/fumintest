page {
  height: 100%;
  width: 100%;
  background: #f4f4f4;
}

.container {
  height: 100%;
  width: 100%;
}

.bank-detail {
  background-size: auto 10.5rpx;
  margin-bottom: 90rpx;
}

.bank-detail .bank-no-pic {
  width: 100%;
  height: 226rpx;
  background: url('data:image/png;base64,');
  background-size: cover;
}

.bank-detail .bank-yes-pic {
  width: 100%;
  height: 226rpx;
  background: url('data:image/png;base64,');
  background-size: cover;
}

.bank-detail .bank-detail-container {
  margin: 0rpx 0rpx;
}

.bank-detail .bank-detail-checkbox {
  margin: 0rpx 0rpx;
}

.bank-detail .bank-detail-item-box {
  display: flex;
  flex-direction: column;
  height: 226rpx;
  margin: 20rpx 40rpx;
}

.bank-detail .bank-detail-icon-box {
  margin-top: 40rpx;
  width: 70rpx;
  height: 70rpx;
}

.bank-detail .bank-detail-icon-box .bank-detail-img-box {
  width: 50rpx;
  height: 50rpx;
  display: block;
}

.bank-detail .bank-detail-right-box {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.bank-detail .bank-detail-text-box {
  margin-top: 40rpx;
}

.bank-detail .bank-detail-audit-box {
  display: flex;
  flex-direction: row;
  width: 80%;
  justify-content: flex-start;
}

.bank-detail .bank-detail-right-box .bank-detail-title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}

.bank-detail .bank-detail-audit-box .bank-detail-amount-value {
  font-size: 40rpx;
  color: #5975fe;
  margin: 5rpx 0rpx;
}

.bank-detail .bank-detail-audit-box .bank-detail-amount-name {
  font-size: 24rpx;
  color: #a7a9aa;
}

.bank-detail .bank-detail-audit-box .bank-detail-interest-value {
  font-size: 40rpx;
  color: #666;
  margin: 5rpx 0rpx;
}

.bank-detail .bank-detail-audit-box .bank-detail-interest-name {
  font-size: 24rpx;
  color: #a7a9aa;
}

.bank-detail .bank-detail-audit-name {
  font-size: 30rpx;
  color: #fe3531;
}

.bank-detail .bank-detail--no-audit-name {
  font-size: 30rpx;
  color: #a7a9aa;
}

.bank-detail .bank-detail-amount-unit {
  color: #5975FE;
  font-size: 24rpx;
}