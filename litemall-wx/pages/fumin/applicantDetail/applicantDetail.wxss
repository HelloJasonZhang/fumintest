page {
  height: 100%;
  width: 100%;
  background: #f4f4f4;
}

.container {
  height: 100%;
  width: 100%;
}

.app-detail {
  background-size: auto 10.5rpx;
  margin-bottom: 30rpx;
}

.app-detail .app-detail-item {
  background: #fff;
  border-bottom: solid rgba(167, 169, 170, 0.3) 0.1rpx;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.app-detail .label, .app-detail-iamg .label {
  width: 250rpx;
  font-size: 30rpx;
  margin: 30rpx;
  color: #7c7e80;
}

.app-detail .name, .app-detail-iamg .name {
  font-size: 30rpx;
  margin: 30rpx;
  color: #333;
}

.app-detail-iamg {
  margin-top: 30rpx;
  padding-top: 10rpx;
  background: #fff;
}

.app-detail-iamg .app-iamge-detail {
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  border-bottom: solid rgba(167, 169, 170, 0.3) 0.1rpx
}

.app-detail-iamg .img-box {
  width: 300rpx;
  height: 300rpx;
}

.app-detail-iamg  .icon-url {
  width: 200rpx;
  height: 200rpx;
  display: block;
}
